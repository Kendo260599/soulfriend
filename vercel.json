{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://soulfriend-api.onrender.com/api/:path*"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://soulfriend-api.onrender.com"
  }
}


